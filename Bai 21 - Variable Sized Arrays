#include <bits/stdc++.h>
using namespace std;
int main(){
    int n, q; cin >> n >> q;
    vector<vector<int>> v(n);
    int a, num;
    for (int i = 0; i < n; i++){
        cin >> a;
        for (int j = 0; j < a; j++){
            cin >> num;
            v[i].push_back(num);
        }
    }int mot, hai;
    for(int j = 0; j < q; j++){
        cin >> mot >> hai;
        cout << v[mot][hai] << endl;
    }
    return 0;
}
