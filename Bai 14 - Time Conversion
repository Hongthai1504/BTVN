#include <bits/stdc++.h>
using namespace std;
int main(){
    string str;
    cin >> str;
    if (str[8] == 'P'){
    if(str[0] == '0' && str[1] == '1') {str[0] = '1'; str[1] = '3';}
    if(str[0] == '0' && str[1] == '2') {str[0] = '1'; str[1] = '4';}
    if(str[0] == '0' && str[1] == '3') {str[0] = '1'; str[1] = '5';}
    if(str[0] == '0' && str[1] == '4') {str[0] = '1'; str[1] = '6';}
    if(str[0] == '0' && str[1] == '5') {str[0] = '1'; str[1] = '7';}
    if(str[0] == '0' && str[1] == '6') {str[0] = '1'; str[1] = '8';}
    if(str[0] == '0' && str[1] == '7') {str[0] = '1'; str[1] = '9';}
    if(str[0] == '0' && str[1] == '8') {str[0] = '2'; str[1] = '0';}
    if(str[0] == '0' && str[1] == '9') {str[0] = '2'; str[1] = '1';}
    if(str[0] == '1' && str[1] == '0') {str[0] = '2'; str[1] = '2';}
    if(str[0] == '1' && str[1] == '1') {str[0] = '2'; str[1] = '3';}  
    }
    if (str[8] == 'A'){
        if (str[0] == '1' && str[1] == '2'){str[0] = '0'; str[1] ='0';}
    }
    for (int i = 0; i <= 7; i++){
        cout << str[i];
    }
    return 0;
}
