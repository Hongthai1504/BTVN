#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    vector <int> v;
    cin >> n;
    for (int i = 0; i < n; i++){
        int num;
        cin >> num;
        v.push_back(num);
    }
    sort(v.begin(),v.end());
     for (auto i = v.begin(); i != v.end(); ++i) 
        cout << *i << " "; 
    return 0;
}
